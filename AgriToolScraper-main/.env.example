# AgriTool Scraper Environment Configuration
# Copy this file to .env and fill in your values

# Supabase Configuration (Required for database upload)
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key-here

# Scraper Configuration (Optional)
TARGET_URL=https://www.afir.info/
MAX_PAGES=0
DRY_RUN=false
BROWSER=chrome

# Rate Limiting (Optional)
MIN_DELAY=2
MAX_DELAY=5
MAX_WORKERS=3

# Logging (Optional)
LOG_LEVEL=INFO
LOG_FILE=data/logs/scraper.log