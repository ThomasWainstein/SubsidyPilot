# FranceAgriMer Scraper Environment Configuration
# Copy this file to .env and fill in your values

# Supabase Configuration (Required for database upload)
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_SERVICE_KEY=your_service_role_key_here

# Scraper Configuration (Optional)
TARGET_URL=https://www.franceagrimer.fr/Accompagner/Dispositifs-par-filiere/Aides-nationales
MAX_PAGES=0
DRY_RUN=false
BROWSER=chrome

# Rate Limiting (Optional)
MIN_DELAY=2
MAX_DELAY=5
MAX_WORKERS=3

# Logging (Optional)
LOG_LEVEL=INFO
LOG_FILE=data/logs/scraper.log